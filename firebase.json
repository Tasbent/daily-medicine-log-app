{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/getFirebaseConfig",
        "function": "getFirebaseConfig"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
